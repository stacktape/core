{
  "typescript.tsdk": "./node_modules/typescript/lib",
  "terminal.integrated.profiles.windows": {
    "pwsh7": {
      "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
      "args": []
    }
  },
  "eslint.workingDirectories": [
    { "directory": "./console-app" },
    { "directory": "./src" },
    { "directory": "./scripts" },
    { "directory": "./shared" }
  ],
  "terminal.integrated.defaultProfile.windows": "pwsh7",
  "search.exclude": {
    "**/@generated/**/*": true,
    "**/__stacktape-dist/**/*": true,
    "**/__source-maps/**/*": true,
    "**/.stacktape/**/*": true,
    "**/__publish-folder/**/*": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/__dev-bin/**/*": true
  },
  "java.configuration.updateBuildConfiguration": "automatic",
  "python.formatting.provider": "autopep8",
  "search.excludeFolders": [".git", "node_modules", "bower_components", "path/to/other/folder/to/exclude"],
  "terminal.integrated.env.windows": {
    "NODE_OPTIONS": "--max-old-space-size=32768"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "explorer.openEditors.visible": 1,
  "vsicons.presets.nestjs": true
}
